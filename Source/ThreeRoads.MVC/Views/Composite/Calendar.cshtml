<link href="@Url.Content("~/Libraries/fullCalendar/css/fullcalendar.css")" rel="stylesheet" type="text/css" media="screen" />
<script src="@Url.Content("~/Libraries/fullCalendar/js/fullcalendar.min.js")" type="text/javascript"></script>
@{
    Layout = "~/Views/Shared/SiteLayout.cshtml";
}
<div id="calendarEventLink">
    @Html.ActionLink("Event View", "Events", "Dynamic")
</div>

<div id='calendar'></div>

<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {

        $('#calendar').fullCalendar({
            events: '/Academy/CalendarEvents',
            weekends: false, // will hide Saturdays and Sundays
            dayClick: function () {
                alert('a day has been clicked!');
            },
            eventMouseOver: function (event, jsEvent, view) {
                alert('mouseover ' + event.title);
            },
            eventRender: function (event, element) {

            }
        })

    });
</script>
