@model IEnumerable<ThreeRoads.Data.Models.Download>


<div>
    @Html.ActionLink("Add New Download", "Create", "Download")
</div>

<table>
    <tr>
        <th></th>
        <th>
            Name
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
        <td>
            @item.Title
        </td>
     </tr>
}

</table>
