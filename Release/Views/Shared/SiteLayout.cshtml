<!DOCTYPE html>
<html>
<head>
    <title>Legacy Classical Christian Academy</title>
    <meta http-equiv="Content-Type" content="application/xhtml+xml; charset=utf-8" />
    <meta name="keywords" content="Classical, Christian, Academy, Phoenix, Arizona, Legacy, School, Scottsdale, private school, Phoenix Private School, Scottsdale Private School, North Phoenix, Christian School, Christian Private School, classical school, Northeast Phoenix, curriculum, tuition, Great Thinkers, literature, 85032, 85254, 85255" />
    <meta name="description" content="private Christian school serving Phoenix and Scottsdale Arizona" />
    <meta name="robots" content="index, follow" />
    <link rel="shortcut icon" href="@Url.Content("~/content/image/favicon.ico")" type="image/x-icon" />
    <link href="@Url.Content("~/content/css/layout.css")" rel="stylesheet" type="text/css" media="screen" />
    <link href="@Url.Content("~/content/css/reset.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/content/css/site.css")" rel="stylesheet" type="text/css" media="screen" />
    <link href="@Url.Content("~/content/css/content.css")" rel="stylesheet" type="text/css" media="screen" />
    <link href="@Url.Content("~/content/css/print.css")" rel="stylesheet" type="text/css" media="print" />
    <script src="@Url.Content("~/content/script/jquery/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/content/script/jquery/jquery-ui-1.8.9.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/content/script/jquery/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/content/script/ajax/microsoftajax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/content/script/mvc/microsoftmvcajax.js")" type="text/javascript"></script>
</head>
<body>
    <div id="wrapper">
<div id="header"> 
            <div id="logo">
                <a href="@Url.Content("~/")"></a>
            </div>
            <div id="titlearea">
                <div id="title"></div>
                <div id="searchbar">
                    @Html.Partial("_SearchBar")
                </div>
            </div>
            
            <div id="quickInfo">
                @Html.Partial("_QuickInfo")
            </div>
</div> 
<div id="colmask"> 
	<div id="colmid"> 
		<div id="colright"> 
			<div id="col1wrap"> 
				<div id="col1pad"> 
					<div id="col1"> 
						<!-- Column 1 start --> 
                        <div id="maincontent">
                            @RenderBody()
                        </div>						
                        <!-- Column 1 end --> 
					</div> 
				</div> 
			</div> 
			<div id="col2"> 
				<!-- Column 2 start --> 
                <div class="navcontainer">	      
                    <div id="sidenav">  
                            @Html.Partial("_SideNav")
                    </div>
                </div>  
            	<!-- Column 2 end --> 
			</div> 
			<div id="col3"> 
				<!-- Column 3 start --> 
                <div class="navcontainer">            
                    <div id="imagelinkcontainer">
                        @Html.Partial("_ImageLinks")
                    </div>
                </div>
            <!-- Column 3 end --> 
			</div> 
		</div> 
	</div> 
</div> 
        <div id="footerNav">
            @Html.Partial("_FooterNav")

        </div>

<div id="footer"> 
                    <span id="copyright">© @DateTime.Today.Year Legacy Classical Christian Academy</span>
            <span id="logindisplay">
                @Html.Partial("_LogOn")
            </span>
</div> 


    </div>

</body>
</html>

