@model IEnumerable<ThreeRoads.Data.Models.Resource>

<div>
    @Html.ActionLink("Create New Resource", "Create", "Resource")
</div>

<table>
    <tr>
        <th></th>
        <th>
            Title
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
        <td>
            @item.Title
        </td>
    </tr>
}

</table>

